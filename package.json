    "test": "yarn build && wtr --coverage",
    "test:watch": "wtr --watch"
  },
  "devDependencies": {
    "@open-wc/testing": "^2.5.32",
    "@web/dev-server": "^0.0.25",
    "@web/test-runner": "^0.10.0",
